alias alias!="vi ~/dotfiles/env/.alias;. ~/dotfiles/env/.alias"
alias hosts='sudo vi /etc/hosts'
alias log5='git log --oneline | head -n5'
alias ls="ls -lhoG"
alias pretty_log="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
alias profile="vi ~/.bash_profile && . ~/.bash_profile" 
alias recent_branches="git reflog --date=relative | grep 'moving' | sed -e 's/checkout: moving from/_/' | sed -e 's/\(HEAD@\)//'|  sed -e 's/[{}:]//g' | head -n10 | uniq"
alias snip!="vi ~/dotfiles/env/.snippets"
alias snip="cat ~/dotfiles/env/.snippets"
alias dev="cd ~/dev/brighttag"
